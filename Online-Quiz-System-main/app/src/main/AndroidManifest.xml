<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.se.onlinequizsystem">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.OnlineQuizSystem">
        <activity android:name=".StudentViewQuizActivity" />
        <activity android:name=".InstructorQuizGradeActivity" />
        <activity android:name=".InstructorQuizGenerationActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".InstructorAddEditQuestionActivity" />
        <activity android:name=".InstructorQuizListActivity" />
        <activity android:name=".InstructorQuestionBankListActivity" />
        <activity android:name=".InstructorQuizFeedbackActivity" />
        <activity android:name=".StudentQuizListActivity" />
        <activity android:name=".StudentAttemptQuizActivity" />
    </application>

</manifest>